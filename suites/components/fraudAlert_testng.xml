<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Fraud Alert" configfailurepolicy="continue" parallel="methods">
    <groups>
        <run>
            <exclude name="ecc"/>
        </run>
    </groups>
    <test verbose="1" name="Fraud Alert" enabled="true" preserve-order="true" thread-count="6">
            <packages>
                <package name="com.scalepoint.automation.tests.fraudAlert"/>
            </packages>
    </test>

    <listeners>
        <listener class-name="com.scalepoint.automation.utils.testng.RetryListener"/>
    </listeners>
</suite>